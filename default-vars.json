    "host": "0.0.0.0",
    "port": 1883,
    "httpPort": 1884
  },
  "debug": {
    "format": "json",
    "level": 4,
    "filter": "DEBUG=Î»*,*tradle*,-@tradle/dynamodb"
  },
  "stackParameters": {
    "BlockchainNetwork": "ethereum:rinkeby",
